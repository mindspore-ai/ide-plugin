{
  "name": "mindspore-dev-toolkit",
  "displayName": "MindSpore Dev Toolkit",
  "description": "Add AI powered code completions to your VS Code",
  "version": "2.0.0",
  "publisher": "MindSpore",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*",
    "onView: TreeViewTest"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mindsporeCompletion.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "mindspore.scanLocalFile",
        "title": "扫描本地文件"
      },
      {
        "command": "TreeViewTest_One.refreshEntry",
        "title": "Refresh",
        "icon": "images/refresh.svg"
        
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "TreeViewTest",
          "title": "Project TreeView",
          "icon": "images/containerIcon.svg"
        }
      ]
    },
    "views": {
      "TreeViewTest": [
        {
          "id": "TreeViewTest_One",
          "name": "File TreeView"
        }
      ]
    },
    "menus": {
      "editor/context": [
        {
          "command": "mindspore.scanLocalFile",
          "group": "navigation"
        }
      ],
      "view/title": [
        {
          "command": "TreeViewTest_One.refreshEntry",
          "when": "view == TreeViewTest_One",
          "grout": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/google-protobuf": "^3.15.6",
    "@types/marked": "^5.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.8",
    "@types/js-yaml": "^4.0.5",
    "axios": "^1.3.3",
    "compressing": "^1.7.0",
    "google-protobuf": "^3.21.2",
    "js-yaml": "^4.1.0",
    "log4js": "^6.7.1",
    "markdown-table": "^3.0.3",
    "marked": "^5.0.5"
  }
}
