{
  "name": "mindspore-dev-toolkit",
  "displayName": "MindSpore Dev Toolkit",
  "description": "Add AI powered code completions to your VS Code",
  "version": "2.1.0",
  "publisher": "MindSpore",
  "icon": "assets/MindSpore-logo.png",
  "engines": {
    "vscode": "^1.69.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*",
    "onView: TreeViewTest"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mindsporeCompletion.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "mindspore.scanLocalFile",
        "title": "API Scan"
      },
      {
        "command": "mindspore.refreshEntry",
        "title": "Refresh",
        "icon": "$(refresh)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "MindSporeActivityBar",
          "title": "API Scan",
          "icon": "icon.svg"
        },
        {
          "id": "MindSporeApiSearchBar",
          "title": "API Search",
          "icon": "mindspore_search_api.svg"
        }
      ]
    },
    "views": {
      "MindSporeActivityBar": [
        {
          "id": "MindSporeTreeView",
          "name": "API Scan"
        }
      ],
      "MindSporeApiSearchBar": [
        {
          "id": "MindSporeSearchApiView",
          "name": "搜索API",
          "type": "webview"
        }
      ]
    },
    "menus": {
      "editor/context": [
        {
          "command": "mindspore.scanLocalFile",
          "group": "navigation",
          "when": "resourceLangId == python"
        }
      ],
      "view/title": [
        {
          "command": "mindspore.refreshEntry",
          "when": "view == MindSporeTreeView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.2.13",
    "@types/glob": "^8.0.1",
    "@types/google-protobuf": "^3.15.6",
    "@types/marked": "^5.0.0",
    "@types/mocha": "^10.0.1",
    "@types/vscode": "^1.69.1",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "web-tree-sitter": "^0.20.8",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.8",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^16.18.36",
    "@vscode/vsce": "^2.19.0",
    "axios": "^1.3.3",
    "compressing": "^1.7.0",
    "google-protobuf": "^3.21.2",
    "js-yaml": "^4.1.0",
    "log4js": "^6.7.1",
    "markdown-table": "^3.0.3",
    "marked": "^5.0.5",
    "vsce": "^2.15.0"
  }
}
